{
  "name": "@sckt/translate",
  "version": "1.0.0",
  "description": "an opinionated API for translator engines (fork of github:franciscop/translate)",
  "homepage": "https://github.com/Socketlike/translate",
  "repository": {
    "type": "git",
    "url": "https://github.com/Socketlike/translate"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": {
    "url": "https://github.com/Socketlike/translate/issues"
  },
  "author": "Evelyn <evelynxii.xt@gmail.com> (https://socketlike.github.io/)",
  "license": "MIT",
  "keywords": [
    "translate",
    "languages",
    "i18n",
    "internationalization",
    "async",
    "google",
    "yandex"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "node scripts/build.mjs",
    "check": "tsc --noEmit",
    "prettier:check": "prettier ./src --check",
    "eslint:check": "eslint ./src",
    "prettier:fix": "prettier ./src --write",
    "eslint:fix": "eslint ./src --fix",
    "lint": "pnpm run prettier:check && pnpm run eslint:check && pnpm run check",
    "lint:fix": "pnpm run prettier:fix && pnpm run eslint:fix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.54.0",
    "eslint-config-dmitmel": "github:dmitmel/eslint-config-dmitmel",
    "eslint-plugin-node": "^11.1.0",
    "iso-639-1": "^3.1.0",
    "memory-cache": "^0.2.0",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/memory-cache": "^0.2.5",
    "@types/node": "^20.9.2",
    "type-fest": "^4.8.2"
  }
}
